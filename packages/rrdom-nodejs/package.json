{
  "name": "rrdom-nodejs",
  "version": "2.0.0-alpha.16",
  "scripts": {
    "dev": "vite build --watch",
    "build": "yarn turbo run prepublish",
    "check-types": "tsc -noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublish": "tsc -noEmit && vite build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "keywords": [
    "rrweb",
    "rrdom-nodejs"
  ],
  "license": "MIT",
  "type": "module",
  "main": "./dist/rrdom-nodejs.umd.cjs",
  "module": "./dist/rrdom-nodejs.js",
  "unpkg": "./dist/rrdom-nodejs.umd.cjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rrdom-nodejs.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/rrdom-nodejs.umd.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "devDependencies": {
    "@types/cssom": "^0.4.1",
    "@types/cssstyle": "^2.2.1",
    "@types/nwsapi": "^2.2.2",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "compare-versions": "^4.1.3",
    "eslint": "^8.15.0",
    "puppeteer": "^9.1.1",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cssom": "^0.5.0",
    "cssstyle": "^2.3.0",
    "nwsapi": "^2.2.0",
    "rrdom": "^2.0.0-alpha.16",
    "rrweb-snapshot": "^2.0.0-alpha.16"
  }
}
