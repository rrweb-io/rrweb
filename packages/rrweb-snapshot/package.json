{
  "name": "rrweb-snapshot",
  "version": "2.0.1-alpha.19",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "description": "rrweb's component to take a snapshot of DOM, aka DOM serializer",
  "scripts": {
    "prepare": "npm run prepack",
    "prepack": "npm run build",
    "retest": "vitest run",
    "test": "yarn build && vitest run",
    "test:watch": "vitest watch",
    "retest:update": "vitest run --update",
    "test:update": "yarn build && vitest run --update",
    "bench": "vite build && vitest bench",
    "dev": "vite build --watch",
    "build": "yarn turbo prepublish -F rrweb-snapshot",
    "check-types": "tsc --noEmit",
    "prepublish": "yarn check-types && vite build",
    "lint": "yarn eslint src"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Midpath-Software/rrweb.git"
  },
  "keywords": [
    "rrweb",
    "snapshot",
    "DOM"
  ],
  "main": "./dist/rrweb-snapshot.umd.cjs",
  "module": "./dist/rrweb-snapshot.js",
  "unpkg": "./dist/rrweb-snapshot.umd.cjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rrweb-snapshot.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/rrweb-snapshot.umd.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "author": "gnpaone@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Midpath-Software/rrweb/issues"
  },
  "homepage": "https://github.com/Midpath-Software/rrweb/tree/master/packages/rrweb-snapshot#readme",
  "devDependencies": {
    "@rrweb/types": "^2.0.1-alpha.19",
    "@rrweb/utils": "^2.0.1-alpha.19",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.15.11",
    "@types/puppeteer": "^5.4.4",
    "puppeteer": "^17.1.3",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "postcss": "^8.4.38"
  }
}
