{
  "name": "rrdom",
  "version": "2.0.1-alpha.19",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/Midpath-Software/rrweb/tree/main/packages/rrdom#readme",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rrdom.cjs",
  "module": "./dist/rrdom.js",
  "unpkg": "./dist/rrdom.umd.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/rrdom.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/rrdom.cjs"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Midpath-Software/rrweb.git"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "yarn turbo run prepublish",
    "check-types": "tsc -noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublish": "tsc -noEmit && vite build",
    "lint": "yarn eslint src/**/*.ts"
  },
  "bugs": {
    "url": "https://github.com/Midpath-Software/rrweb/issues"
  },
  "devDependencies": {
    "@rrweb/types": "^2.0.1-alpha.19",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "puppeteer": "^17.1.3",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "rrweb-snapshot": "^2.0.1-alpha.19"
  }
}
